<main class="main">
  <div class="inner">
    <%if user_signed_in? %>
      <div class="greeting">
       <%= link_to "こんにちは、#{current_user.name}さん", user_path(current_user) , class: :greeting__link%> 
      </div>
     <% end %>
    <div class="card__wrapper">
      <%= render partial: 'photo', collection: @photos %>
    </div>
  </div>
</main>
